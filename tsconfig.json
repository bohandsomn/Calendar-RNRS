{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "esnext",
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-native",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    
    "strict": true
  }
}
